<%= render 'header' %>
<h1 class="text-center">Available Midterms</h1>

<table class="table">
  <thead>
    <tr>
      <th>Year</th>
      <th>School</th>
      <th>Coursecode</th>
      <th>Corrected</th>
      <th>Professor</th>
      <th>Semester</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @midterms.each do |midterm| %>
      <tr>
      	<td><%= midterm.year %></td>
        <td><%= midterm.school %></td>
        <td><%= midterm.courseCode %></td>
        <td><%= midterm.corrected %></td>
        <td><%= midterm.professor %></td>
        <td><%= midterm.semester %></td>
        <td><%= link_to 'Show', midterm %></td>
        <td><%= link_to 'Edit', edit_midterm_path(midterm) %></td>
        <td><%= link_to 'Destroy', midterm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Submit a Midterm!', new_midterm_path %>
